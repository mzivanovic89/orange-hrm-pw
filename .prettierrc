{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "arrowParens": "always",
  "semi": true,
  "overrides": [
    {
      "files": ["*.ts"],
      "options": {
        "trailingComma": "es5"
      }
    }
  ]
}
